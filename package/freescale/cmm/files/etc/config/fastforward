# Don't Fast Forward FTP traffic
config fastforward ftp
	option proto		tcp
	option port		21
config fastforward sip
        option proto            udp
        option port             5060
config fastforward pptp
        option proto            tcp
        option port             1723

config fastforward box
        option mark             '2 mask 15'

